<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
    <interface name="org.freedesktop.Akonadi.BalooIndexer">
        <method name="reindexCollection" >
            <arg name="item" type="x" direction="in"/>
        </method>
        <method name="indexedItems" >
            <arg name="item" type="x" direction="in"/>
            <arg type="x" direction="out"/>
        </method>
        <method name="collectionQueue">
            <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="QList&lt;qint64&gt;"/>
            <arg type="ax" direction="out"/>
        </method>
        <signal name="currentCollectionChanged">
            <arg type="x" name="colId" direction="out"/>
        </signal>
    </interface>
</node>
