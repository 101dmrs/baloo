msgstr "Habilita l'indexador de fitxers"

#: main.cpp:77
msgid "Disable the file indexer"
msgstr "Deshabilita l'indexador de fitxers"

#: main.cpp:78
msgid "Start the file indexer"
msgstr "Inicia l'indexador de fitxers"

#: main.cpp:79
msgid "Stop the file indexer"
msgstr "Atura l'indexador de fitxers"

#: main.cpp:80
msgid "Restart the file indexer"
msgstr "Torna a iniciar l'indexador de fitxers"

#: main.cpp:81
msgid "Suspend the file indexer"
msgstr "Suspèn l'indexador de fitxers"

#: main.cpp:82
