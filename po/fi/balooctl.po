#: main.cpp:78
msgid "Start the file indexer"
msgstr "Käynnistä tiedostoindeksoija"

#: main.cpp:79
msgid "Stop the file indexer"
msgstr "Pysäytä tiedostoindeksoija"

#: main.cpp:80
msgid "Restart the file indexer"
msgstr "Käynnistä tiedostoindeksoija uudelleen"

#: main.cpp:81
msgid "Suspend the file indexer"
