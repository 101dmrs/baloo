#: configcommand.cpp:45
msgid "Manipulate the Baloo configuration"
msgstr "Muuta Baloon asetuksia"

#: configcommand.cpp:68
msgid "The config command can be used to manipulate the Baloo Configuration"
msgstr "config-komennolla voi muuttaa Baloon asetuksia"

#: configcommand.cpp:69
msgid "Usage: balooctl config <command>"
msgstr "Käyttö: balooctl config <komento>"

#: configcommand.cpp:70
msgid "Possible Commands:"
msgstr "Mahdolliset komennot:"

#: configcommand.cpp:73
msgid "Add a value to config parameter"
msgstr "Lisää arvo asetusparametriin"

#: configcommand.cpp:74
msgid "Remove a value from a config parameter"
msgstr "Poista arvo asetusparametrista"

#: configcommand.cpp:75
msgid "Show the value of a config parameter"
msgstr "Näytä asetusparametrin arvo"

#: configcommand.cpp:76
msgid "Set the value of a config parameter"
msgstr "Aseta asetusparametrin arvo"

#: configcommand.cpp:77
msgid "Display this help menu"
msgstr "Näytä tämä ohjevalikko"

#: configcommand.cpp:83
msgid "The following configuration options may be listed:"
msgstr "Seuraavat asetukset voi listata:"

#: configcommand.cpp:85 configcommand.cpp:356
msgid "Controls if Baloo indexes hidden files and folders"
msgstr "Hallitsee sitä, indeksoiko Baloo piilotiedostoja ja -kansioita"

#: configcommand.cpp:86
msgid "Controls if baloo indexes file content."
msgstr "Hallitsee sitä, indeksoiko Baloo tiedostojen sisältöä."

#: configcommand.cpp:87 configcommand.cpp:144 configcommand.cpp:237
msgid "The list of folders which Baloo indexes"
msgstr "Luettelo kansioista, jotka Baloo indeksoi"

#: configcommand.cpp:88 configcommand.cpp:145 configcommand.cpp:238
msgid "The list of folders which Baloo will never index"
msgstr "Luettelo kansioista, joita Baloo ei indeksoi ikinä"

#: configcommand.cpp:89 configcommand.cpp:146 configcommand.cpp:239
msgid "The list of filters which are used to exclude files"
msgstr "Luettelo suodattimista, joita käytetään indeksoinnin ohittamiseen"

#: configcommand.cpp:136 configcommand.cpp:229 configcommand.cpp:348
#: configcommand.cpp:417
msgid "Config parameter could not be found"
msgstr "Asetusparametria ei löydy"

#: configcommand.cpp:142 configcommand.cpp:235 configcommand.cpp:354
msgid "The following configuration options may be modified:"
msgstr "Seuraavia asetuksia voi muuttaa:"

#: configcommand.cpp:154 configcommand.cpp:184 configcommand.cpp:248
#: configcommand.cpp:290
msgid "A folder must be provided"
msgstr "Kansio täytyy antaa"

#: configcommand.cpp:160 configcommand.cpp:190 configcommand.cpp:254
#: configcommand.cpp:296
msgid "Path does not exist"
msgstr "Polkua ei ole olemassa"

#: configcommand.cpp:165 configcommand.cpp:195 configcommand.cpp:259
#: configcommand.cpp:301
msgid "Path is not a directory"
msgstr "Polku ei ole hakemisto"

#: configcommand.cpp:214 configcommand.cpp:332
msgid "A filter must be provided"
msgstr "Suodatin täytyy antaa"

#: configcommand.cpp:220
msgid "is not in list of exclude filters"
msgstr "ei ole ohitussuodatinten listassa"

#: configcommand.cpp:338
msgid "Exclude filter is already in the list"
msgstr "Ohitussuodatin on jo luettelossa"

#: configcommand.cpp:365 configcommand.cpp:392
msgid "Must provide a value"
msgstr "Arvo täytyy antaa"

#: configcommand.cpp:386 configcommand.cpp:413
msgid "Invalid value"
msgstr "Virheellinen arvo"

