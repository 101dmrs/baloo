#: configcommand.cpp:45
msgid "Manipulate the Baloo configuration"
msgstr "Manipular a configuração do Baloo"

#: configcommand.cpp:68
msgid "The config command can be used to manipulate the Baloo Configuration"
msgstr ""
"O comando de configuração pode ser usado para manipular a Configuração do "
"Baloo"

#: configcommand.cpp:69
msgid "Usage: balooctl config <command>"
msgstr "Utilização: balooctl config <comando>"

#: configcommand.cpp:70
msgid "Possible Commands:"
msgstr "Comandos Possíveis:"

#: configcommand.cpp:73
msgid "Add a value to config parameter"
msgstr "Adicionar um valor ao parâmetro de configuração"

#: configcommand.cpp:74
msgid "Remove a value from a config parameter"
msgstr "Remover um valor ao parâmetro de configuração"

#: configcommand.cpp:75
msgid "Show the value of a config parameter"
msgstr "Mostrar o valor do parâmetro de configuração"

#: configcommand.cpp:76
msgid "Set the value of a config parameter"
msgstr "Modificar o valor do parâmetro de configuração"

#: configcommand.cpp:77
msgid "Display this help menu"
msgstr "Mostrar este menu de ajuda"

#: configcommand.cpp:83
msgid "The following configuration options may be listed:"
msgstr "Poderá enumerar as seguintes opções de configuração:"

#: configcommand.cpp:85 configcommand.cpp:356
msgid "Controls if Baloo indexes hidden files and folders"
msgstr "Controla se o Baloo indexa os ficheiros e pastas escondidos"

#: configcommand.cpp:86
msgid "Controls if baloo indexes file content."
msgstr "Controla se o Baloo indexa o conteúdo dos ficheiros."

#: configcommand.cpp:87 configcommand.cpp:144 configcommand.cpp:237
msgid "The list of folders which Baloo indexes"
msgstr "A lista de pastas que o Baloo indexa"

#: configcommand.cpp:88 configcommand.cpp:145 configcommand.cpp:238
msgid "The list of folders which Baloo will never index"
msgstr "A lista de pastas que o Baloo nunca irá indexar"

#: configcommand.cpp:89 configcommand.cpp:146 configcommand.cpp:239
msgid "The list of filters which are used to exclude files"
msgstr "A lista de filtros que são usados para excluir os ficheiros"

#: configcommand.cpp:136 configcommand.cpp:229 configcommand.cpp:348
#: configcommand.cpp:417
msgid "Config parameter could not be found"
msgstr "Não foi encontrado o parâmetro de configuração"

#: configcommand.cpp:142 configcommand.cpp:235 configcommand.cpp:354
msgid "The following configuration options may be modified:"
msgstr "Poderá modificar as seguintes opções de configuração:"

#: configcommand.cpp:154 configcommand.cpp:184 configcommand.cpp:248
#: configcommand.cpp:290
msgid "A folder must be provided"
msgstr "Deverá indicar uma pasta"

#: configcommand.cpp:160 configcommand.cpp:190 configcommand.cpp:254
#: configcommand.cpp:296
msgid "Path does not exist"
msgstr "A localização não existe"

#: configcommand.cpp:165 configcommand.cpp:195 configcommand.cpp:259
#: configcommand.cpp:301
msgid "Path is not a directory"
msgstr "A localização não é uma pasta"

#: configcommand.cpp:214 configcommand.cpp:332
msgid "A filter must be provided"
msgstr "Deverá indicar um filtro"

#: configcommand.cpp:220
msgid "is not in list of exclude filters"
msgstr "não está na lista de filtros de exclusão"

#: configcommand.cpp:338
msgid "Exclude filter is already in the list"
msgstr "O filtro de exclusão já existe na lista"

#: configcommand.cpp:365 configcommand.cpp:392
msgid "Must provide a value"
msgstr "Deverá indicar um valor"

#: configcommand.cpp:386 configcommand.cpp:413
msgid "Invalid value"
msgstr "Valor inválido"

#: main.cpp:66
msgid "balooctl"
msgstr "balooctl"

#: main.cpp:67
msgid "Vishesh Handa"
msgstr "Vishesh Handa"

#: main.cpp:67
msgid "Maintainer"
msgstr "Manutenção"

#: main.cpp:73
msgid "The command to execute"
msgstr "O comando a executar"

#: main.cpp:75
msgid "Print the status of the indexer"
msgstr "Imprime o estado do sistema de indexação"

#: main.cpp:76
msgid "Enable the file indexer"
msgstr "Activar a indexação de ficheiros"

#: main.cpp:77
msgid "Disable the file indexer"
msgstr "Desactivar a indexação de ficheiros"

#: main.cpp:78
msgid "Start the file indexer"
msgstr "Iniciar a indexação de ficheiros"

#: main.cpp:79
msgid "Stop the file indexer"
msgstr "Parar a indexação de ficheiros"

#: main.cpp:80
msgid "Restart the file indexer"
msgstr "Reiniciar a indexação de ficheiros"

#: main.cpp:81
msgid "Suspend the file indexer"
msgstr "Suspender a indexação de ficheiros"

#: main.cpp:82
msgid "Resume the file indexer"
msgstr "Prosseguir a indexação de ficheiros"

#: main.cpp:83
msgid "Check for any unindexed files and index them"
msgstr "Procurar por ficheiros não indexados e indexá-los"

#: main.cpp:84
msgid "Index the specified files"
msgstr "Indexar os ficheiros indicados"

#: main.cpp:85
msgid "Forget the specified files"
msgstr "Esquecer os ficheiros indicados"

#: main.cpp:86
msgid "Modify the Baloo configuration"
msgstr "Modificar a configuração do Baloo"

#: main.cpp:117
msgid "Baloo is currently disabled. To enable, please run \"balooctl enable\""
msgstr ""
"O Baloo está desactivado de momento. Para activar, execute por favor "
"\"balooctl enable\""
